{
    "name": "jaxwilko/laravel-ups-api",
    "description": "A small Laravel's wrapper for the PHP UPS API library",
    "keywords": [
        "laravel",
        "framework",
        "UPS",
        "Laravel UPS Api",
        "Laravel-Ups-Api",
        "Pierre Tondereau",
        "Ptondereau"
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "jaxwilko/ups-api": "^1.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Ptondereau\\LaravelUpsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ptondereau\\Tests\\LaravelUpsApi\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Tondereau",
            "email": "pierre.tondereau@gmail.com"
        },
        {
            "name": "Jack Wilkinson",
            "email": "me@jackwilky.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ptondereau\\LaravelUpsApi\\UpsApiServiceProvider"
            ]
        }
    }
}
